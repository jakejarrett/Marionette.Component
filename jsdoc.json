{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": "./build/",
    "includePattern": ".es6$",
    "excludePattern": "(node_modules/|docs|.min)"
  },
  "plugins": [
    "plugins/markdown"
  ],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}